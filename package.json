{
  "name": "jods",
  "version": "1.1.0",
  "description": "A minimal, reactive JSON state layer for Node.js and the browser",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "require": "./dist/react.cjs",
      "types": "./dist/react.d.ts"
    },
    "./preact": {
      "import": "./dist/preact.js",
      "require": "./dist/preact.cjs",
      "types": "./dist/preact.d.ts"
    },
    "./remix": {
      "import": "./dist/remix.js",
      "require": "./dist/remix.cjs",
      "types": "./dist/remix.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "docs:dev": "cd docs && pnpm start",
    "docs:build": "cd docs && pnpm build",
    "docs:deploy": "cd docs && GIT_USER=clamstew pnpm deploy",
    "docs:screenshot": "cd docs && pnpm screenshot",
    "docs:screenshot:localhost": "cd docs && pnpm screenshot:localhost",
    "docs:screenshot:production": "cd docs && pnpm screenshot:production",
    "docs:screenshot:component": "cd docs && pnpm screenshot:component",
    "docs:screenshot:component:localhost": "cd docs && pnpm screenshot:component:localhost",
    "docs:screenshot:component:production": "cd docs && pnpm screenshot:component:production",
    "docs:screenshot:fix": "node docs/scripts/screenshot-fix.mjs",
    "docs:screenshot:fix:baseline": "node docs/scripts/screenshot-fix.mjs --baseline",
    "docs:screenshot:baseline": "cd docs && pnpm screenshot:baseline",
    "docs:screenshot:cleanup": "cd docs && pnpm screenshot:cleanup",
    "docs:screenshot:cleanup:keep-latest": "cd docs && pnpm screenshot:cleanup:keep-latest",
    "docs:screenshot:cleanup:sections": "cd docs && pnpm screenshot:cleanup:sections",
    "docs:screenshot:cleanup:sections:keep": "cd docs && pnpm screenshot:cleanup:sections:keep",
    "docs:screenshot:cleanup:sections:dry-run": "cd docs && pnpm screenshot:cleanup:sections:dry-run",
    "docs:screenshot:sections:homepage": "node docs/scripts/homepage-sections.mjs",
    "docs:screenshot:sections:homepage:baseline": "node docs/scripts/homepage-sections.mjs --baseline",
    "docs:screenshot:sections:generate-selectors": "node docs/scripts/generate-selectors.mjs",
    "docs:screenshot:sections:use-selectors": "node docs/scripts/use-selectors.mjs",
    "docs:screenshot:sections:use-selectors:baseline": "node docs/scripts/use-selectors.mjs --baseline",
    "docs:screenshot:sections:regenerate": "node docs/scripts/use-selectors.mjs --regenerate",
    "docs:screenshot:sections:remix": "node docs/scripts/remix-section.mjs",
    "docs:screenshot:sections:remix:baseline": "node docs/scripts/remix-section.mjs --baseline",
    "docs:screenshot:unified": "node docs/scripts/screenshot-unified.mjs",
    "docs:screenshot:unified:baseline": "node docs/scripts/screenshot-unified.mjs --baseline",
    "docs:screenshot:unified:localhost": "BASE_URL=http://localhost:3000 node docs/scripts/screenshot-unified.mjs",
    "docs:screenshot:unified:components": "node docs/scripts/screenshot-unified.mjs --mode=components",
    "docs:screenshot:unified:sections": "node docs/scripts/screenshot-unified.mjs --mode=sections",
    "docs:screenshot:unified:remix": "node docs/scripts/screenshot-unified.mjs --mode=remix"
  },
  "keywords": [
    "state",
    "reactive",
    "store",
    "json",
    "observable"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@remix-run/react": "^2.16.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.1",
    "@types/react": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.57.0",
    "jsdom": "^26.1.0",
    "preact": "^10.26.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "terser": "^5.39.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2",
    "zod": "^3.24.3"
  },
  "peerDependencies": {
    "@remix-run/react": ">=1.0.0",
    "preact": ">=10.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "@remix-run/react": {
      "optional": true
    }
  }
}
