{
  "name": "jods",
  "version": "0.1.2",
  "description": "A minimal, reactive JSON state layer for Node.js and the browser",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "require": "./dist/react.cjs",
      "types": "./dist/react.d.ts"
    },
    "./preact": {
      "import": "./dist/preact.js",
      "require": "./dist/preact.cjs",
      "types": "./dist/preact.d.ts"
    },
    "./remix": {
      "import": "./dist/remix.js",
      "require": "./dist/remix.cjs",
      "types": "./dist/remix.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "docs:dev": "cd docs && pnpm start",
    "docs:build": "cd docs && pnpm build",
    "docs:deploy": "cd docs && GIT_USER=clamstew pnpm deploy"
  },
  "keywords": [
    "state",
    "reactive",
    "store",
    "json",
    "observable"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@remix-run/react": "^2.16.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.1",
    "@types/react": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.57.0",
    "jsdom": "^26.1.0",
    "preact": "^10.26.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "terser": "^5.39.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2",
    "zod": "^3.24.3"
  },
  "peerDependencies": {
    "@remix-run/react": ">=1.0.0",
    "preact": ">=10.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "@remix-run/react": {
      "optional": true
    }
  }
}
